{
  "name": "tui-grid",
  "version": "4.8.1",
  "description": "TOAST UI Grid : Powerful data grid control supported by TOAST UI",
  "main": "dist/tui-grid.js",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "check-types": "tsc --project ./cypress/tsconfig.json",
    "validate": "npm run check-types && npm run lint",
    "start": "npm run validate && webpack-dev-server",
    "build": "npm run validate && (webpack --mode=production & webpack --mode=production --minify) && node tsBannerGenerator.ts",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test": "start-server-and-test start http://localhost:8000 cy:open",
    "test:ci": "start-server-and-test start http://localhost:8000 cy:run",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "ts2js": "tsc --outDir tmpdoc --sourceMap false --target ES2015 --noEmit false",
    "doc:dev": "npm run ts2js && tuidoc --dev",
    "doc": "npm run ts2js && tuidoc",
    "tuie": "tuie"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nhn/tui.grid.git"
  },
  "keywords": [
    "nhn",
    "tui",
    "application",
    "grid",
    "tui-grid"
  ],
  "files": [
    "dist",
    "index.d.ts"
  ],
  "author": "NHN. FE Development Lab",
  "license": "MIT",
  "devDependencies": {
    "@cypress/watch-preprocessor": "^1.0.0",
    "@cypress/webpack-preprocessor": "^4.0.3",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/addon-notes": "^5.2.5",
    "@storybook/html": "^5.2.5",
    "@types/node": "^12.0.0",
    "@types/webpack-env": "^1.13.8",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "css-loader": "^2.1.1",
    "cypress": "^3.8.1",
    "cypress-plugin-tab": "^1.0.3",
    "cypress-promise": "^1.0.2",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-tui": "^3.0.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.12.4",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "start-server-and-test": "^1.10.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "ts-loader": "^6.2.1",
    "tui-release-notes": "git+https://github.com/nhnent/tui.release-notes.git",
    "typescript": "^3.7.2",
    "utility-types": "^3.4.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "preact": "^8.4.2",
    "tslib": "^1.10.0",
    "tui-date-picker": "^3.3.3",
    "tui-pagination": "^3.3.2"
  }
}
